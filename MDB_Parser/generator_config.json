{
    "$schema": "generator_config.schema.json",
    "_comment": "Configuration for IL2CPP wrapper generator. Edit this file to customize for your game.",
    
    "output": {
        "namespace_prefix": "GameSDK",
        "output_directory": "MDB_Core/Generated",
        "file_prefix": "GameSDK"
    },
    
    "skip_namespaces": {
        "_comment": "Exact namespace names to skip. Universal .NET/Unity namespaces are always skipped.",
        "custom": []
    },
    
    "skip_namespace_prefixes": {
        "_comment": "Namespace prefixes to skip. All namespaces starting with these will be excluded.",
        "custom": []
    },
    
    "skip_types": {
        "_comment": "Specific type names to skip regardless of namespace.",
        "custom": []
    },
    
    "skip_base_types": {
        "_comment": "Types that cannot be used as base classes (will skip wrapper generation for derived types).",
        "custom": []
    },
    
    "auto_detect_third_party": {
        "_comment": "If true, automatically detect and skip common third-party library namespaces found in the dump.",
        "enabled": true,
        "patterns": [
            "Photon",
            "ExitGames",
            "Steamworks",
            "Facepunch",
            "Discord",
            "DiscordRPC",
            "PlayFab",
